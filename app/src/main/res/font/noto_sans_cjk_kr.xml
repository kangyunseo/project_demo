<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- normal -->
    <font
        android:font="@font/noto_sans_cjk_kr_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/noto_sans_cjk_kr_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- italic -->
    <font
        android:font="@font/noto_sans_cjk_kr_light"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/noto_sans_cjk_kr_light"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <!-- blod -->
    <font
        android:font="@font/noto_sans_cjk_kr_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/noto_sans_cjk_kr_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>